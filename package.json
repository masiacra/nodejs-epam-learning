{
  "name": "nodejs-epam-learning",
  "version": "1.0.0",
  "description": "repository to do hometasks for epam node.js global mentoring",
  "main": "index.js",
  "scripts": {
    "start": "npm run build:live",
    "build": "tsc -p .",
    "build:live": "nodemon --watch 'src/rest-service/**/*.ts' --exec \"ts-node\" src/rest-service/index.ts",
    "create-hook": "husky add .husky/pre-commit \"npm test\"",
    "lint": "eslint src",
    "precommit": "eslint .",
    "prepare": "husky install",
    "task1": "babel ./src/reverse/ -d dist && nodemon ./dist",
    "task2": "babel ./src/read-csv/ -d dist && nodemon ./dist",
    "test": ""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/masiacra/nodejs-epam-learning.git"
  },
  "keywords": [
    "node.js",
    "global",
    "mentoring"
  ],
  "author": "Igor Savelev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/masiacra/nodejs-epam-learning/issues"
  },
  "homepage": "https://github.com/masiacra/nodejs-epam-learning#readme",
  "devDependencies": {
    "@babel/cli": "7.15.7",
    "@babel/core": "7.15.5",
    "@babel/preset-env": "7.15.6",
    "@types/node": "16.10.3",
    "eslint": "7.32.0",
    "eslint-config-node": "4.1.0",
    "eslint-config-prettier": "8.3.0",
    "husky": "7.0.2",
    "lint-staged": "11.1.2",
    "nodemon": "2.0.13",
    "prettier": "2.4.1",
    "ts-node": "10.3.0",
    "typescript": "4.4.3"
  },
  "dependencies": {
    "csvtojson": "2.0.10"
  }
}
